{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "types": ["cypress", "node", "@testing-library/cypress"],
    "isolatedModules": false,
    "noEmit": true,
    "composite": true,
    "jsx": "react-jsx",
    "strict": true
  },
  "include": ["/*.tsx", "/.ts", "**/.js", "/*.jsx",     "../client/src/components//.ts",
  "../client/src/components/**/.tsx",
  "integration//*.ts",
  "integration//*.tsx"]
}
